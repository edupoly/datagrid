html
    head 
        title= 'Welcome to pug'
        link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css' rel='stylesheet')
    body 
        include ./header.pug
        h1= 'This is Pug view Engine'
        h2.bg-success= title
        h3= "my age is : "+age
        ul 
            each val in techs 
                li= val
        ol 
            each contact in contacts
                li= contact.email